{
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-console": 0
  },

  "env": {
    "es6": false,
    "node": true,
    "browser": true
  },

  "globals": {
    "$": true,
    "templates": true
  },

  "extends": "eslint:recommended"
}
